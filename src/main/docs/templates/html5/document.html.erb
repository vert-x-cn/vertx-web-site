<%#encoding:UTF-8%>---
template: page.html
type: docs
sidebar: true
title: <%= doctitle(:sanitize => true) || (attr 'untitled-label') %> - Vert.x
heading: <%= doctitle(:sanitize => true) || (attr 'untitled-label') %>
stylesheet: docs.css
---

<div id="content">
  <div class="container docs-content">
    <div class="row">
      <div class="col-sm-12 col-md-push-9 col-md-3 hidden-xs hidden-sm">
        <div id="sidebar" data-spy="affix">
          <%= converter.convert self, 'outline' %>
        </div>
      </div>
      <div class="col-sm-12 col-md-pull-3 col-md-9">
        <div class="toc hidden-md hidden-lg">
          <h2><%= attr 'toc-title' %></h2>
          <%= converter.convert self, 'outline' %>
        </div>
<% if attr? :modulepath %>
  <a href="https://github.com/okou19900722/vertx-web-site-translation-chinese/tree/master/vertx-translation-stack/<%= attr :modulepath %>"
     class="page-link-to-github"
     target="_blank"
     title="Edit this page on GitHub">
    <i class="github-icon"></i>
    <span class="text">编辑本页</span>
  </a>
<% end %>
        <%= content %>
        <% unless !footnotes? || attr?(:nofootnotes) %>
          <div id="footnotes">
          <% footnotes.each do |fn| %>
            <div class="footnote" id="_footnote_<%= fn.index %>">
              <a href="#_footnoteref_<%= fn.index %>"><%= fn.index %></a>. <%= fn.text %>
            </div>
          <% end %>
        </div>
        <% end %>

        <% unless nofooter %>
          <div id="footer">
            <div id="footer-text">
              <% if attr? :revnumber %>
                <%= %(源文档版本 #{attr :revnumber}) %><br>
              <% end
                if attr? 'last-update-label' %>
                <%= %(上次更新时间 #{attr :docdatetime}) %>
              <% end %>
              <%= (docinfo_content = (docinfo :footer)).empty? ? nil : %(#{docinfo_content}) %>
            </div>
          </div>
        <% end %>
      </div>
    </div>
  </div>
</div>
